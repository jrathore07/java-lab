import java.util.Scanner;
class Main implements Runnable
{
	public static void main(String[] args) {
	    Main t1 =new Main();
	    Thread tc=new Thread(t1);
	    tc.start();
	     Main t2 =new Main();
	    Thread ta=new Thread(t2);
	    ta.start();
	     
	   
	}
	public void run()
	{
	    Scanner sc=new Scanner(System.in);
	    int n=sc.nextInt();
	    for(int i=1;i<=10;i++)
	    
	   System.out.println(n+"*"+i+"="+i*n); 
	}
}

